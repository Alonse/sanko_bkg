                                    +e.SPAN.history-item-content-text-title Дата
                                    +e.history-item-content-text-wrapper
                                        +e.history-item-image-wrapper
                                            +e.IMG.history-item-image(src=require('./../images/date.svg'), alt='')
                                        +e.SPAN.history-item-desc 3 сентября 2020
                                +e.history-item-content-text
                                    +e.SPAN.history-item-content-text-title Время
                                    +e.history-item-content-text-wrapper
                                        +e.history-item-image-wrapper
                                            +e.IMG.history-item-image(src=require('./../images/time.svg'), alt='')
                                        +e.SPAN.history-item-desc 12:00 – 17:00
