mixin slider()
    +b.slider
        .container
            +e.inner
                +e.item
                    +e.item-title Последние события
                    +e.slider
                        +e.navigation
                            +e.button-inner
                                +e.BUTTON.button.events__prev-button(id='prev-button')
                            +e.date
                                +e.date-day 26
                                +e.date-month ФЕВ
                            +e.button-inner
                                +e.BUTTON.button.events__next-button(id='next-button')
                        +e.slide-shadow-wrapper
                            +e.wrapper(id='slides-wrapper')
                                +e.slide.events__slide_active
                                    +e.logo
                                        +e.logo-pic(src=require('./../images/krauta-logo.svg'), alt='')
                                        +e.d-logo(src=require('./../images/d-logo.png'), alt='')
                                    +e.content
                                        +e.description
                                            +e.description-marker.events__description-marker_orange
                                            +e.description-text Проведение презентация прогр. SIY для рук-лей эйчар-функции
                                        +e.details
                                            +e.details-inner
                                                +e.place.events__item_disabled
