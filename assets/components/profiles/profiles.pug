                                +e.link-icon(src=require('./../images/arrow_orange.svg'), alt='')
            +e.online-inner
                .container
                    +e.online-wrapper
                        +e.flick(src=require('./../images/flick.png'), alt='')
                        +e.A.online(href='')
                            +e.H3.online-title Решения и продукты online формата
                            +e.online-subtitle
                                +e.SPAN Принять участие
                                +e.SVG.online-icon(mlns="http://www.w3.org/2000/svg" width="6" height="9" viewBox="0 0 6 9" fill="none")
                                    +e.PATH.dropdown-icon-path(d="M1.5 1.5L4.5 4.5L1.5 7.5" stroke="#B9B9B9" stroke-width="1.3" stroke-linecap="round")